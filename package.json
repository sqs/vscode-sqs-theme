{
  "name": "vscode-sqs-theme",
  "displayName": "SQS Theme",
  "description": "SQS theme for Visual Studio Code",
  "version": "0.0.9",
  "publisher": "sqs",
  "bugs": {
    "url": "https://github.com/sqs/vscode-sqs-theme/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sqs/vscode-sqs-theme"
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "SQS Dark",
        "uiTheme": "vs-dark",
        "path": "./sqs-dark.json"
      }
    ]
  },
  "keywords": [
    "sqs",
    "sqs dark",
    "dark",
    "theme"
  ],
  "scripts": {
    "attach": "./dev.sh attach",
    "eject": "./dev.sh eject",
    "package": "mkdir out && vsce package -o out/vscode-sqs-theme.vsix"
  }
}
